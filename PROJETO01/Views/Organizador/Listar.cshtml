
@model List<PROJETO01.Modelos.Organizador>

<br />
<br />
<br />

<div class="container-fluid bg-light" style="border:solid">
    <br />
    <br />
<h3>
    Cadastro de Organizadores
    <br />
    <a class="btn btn-dark  float-right" href="/Organizador/Adicionar" style="background-color:darkolivegreen">
        ADICIONAR
    
   </a>
    
</h3>

<table class="table table-hover ">
    <thead>
        <tr>
            <th>OrganizadorID</th>
            <th>Nome</th>
            <th>CPF</th>
            <th>Telefone</th>
            <th>Ações</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.OrganizadorID</td>
                <td>@item.Nome</td>
                <td>@item.CPF</td>
                <td>@item.Telefone</td>
                <td>
                    <a href="/Organizador/Editar?OrganizadorID=@item.OrganizadorID" class="btn btn-sm btn-dark">
                        Editar
                    </a>
                    <a href="/Organizador/Excluir?OrganizadorID=@item.OrganizadorID" class="btn btn-sm btn-danger">
                        Excluir
                    </a>
                </td>
            </tr>
        }
    </tbody>
</table>
    </div>
